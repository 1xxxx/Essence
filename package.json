{
  "name": "EssenceMD",
  "version": "1.0.0",
  "description": "Essence Material Design ReactJS",
  "main": "lib/essence.min.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "essence:test": "webpack-dev-server --devtool eval --hot --progress --colors --config templates/essence/js/webpack.config.js  --content-base templates/essence/",
    "essence:live": "NODE_ENV=production webpack -p --config templates/essence/js/webpack.config.js && http-server ./templates/essence/ -p 8080 -a 0.0.0.0 -o",
    "test": "webpack-dev-server --devtool eval --hot --progress --colors --config test/webpack.test.config.js",
    "dev": "webpack-dev-server --devtool eval --hot --progress --colors --config test/webpack.config.js",
    "production": "NODE_ENV=production webpack -p --config test/webpack.production.config.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [
    "react",
    "reactjs"
  ],
  "author": "Hetmann W. Iohan",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.0",
    "essence-core": "*",
    "essence-appbar": "*",
    "essence-bottomsheet": "*",
    "essence-button": "*",
    "essence-card": "*",
    "essence-chip": "*",
    "essence-data-table": "*",
    "essence-dialog": "*",
    "essence-icon": "*",
    "essence-image": "*",
    "essence-input": "*",
    "essence-list": "*",
    "essence-menu": "*",
    "essence-navigation": "*",
    "essence-paper": "*",
    "essence-progress": "*",
    "essence-slider": "*",
    "essence-snackbar": "*",
    "essence-switch": "*",
    "essence-tab": "*",
    "essence-toast": "*",
    "essence-tooltip": "*",
    "essence-touchpad": "*",
    "extract-text-webpack-plugin": "^0.9.1",
    "gulp-less": "^3.0.5",
    "gulp-minify-css": "^1.2.2",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "react-addons-css-transition-group": "^0.14.3",
    "react-addons-transition-group": "^0.14.3",
    "react-addons-update": "^0.14.3",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  }
}
